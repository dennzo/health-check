{
  "name": "dennzo/health-check",
  "license": "MIT",
  "type": "library",
  "description": "A library containing tools for outputting json information for monitoring systems.",
  "keywords": [
    "monitoring",
    "health_check",
    "health",
    "check",
    "json"
  ],
  "authors": [
    {
      "name": "Dennis Barlowe",
      "email": "dennis@dennzo.com",
      "homepage": "https://dennzo.dev",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-json": "*"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Dennzo\\Monitoring\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "symfony/phpunit-bridge": "^5.0",
    "symfony/test-pack": "^1.0"
  }
}
